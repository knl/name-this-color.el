(defun ntc--compute-difference (color-code-1 color-code-2)
  "Computes CIEDE2000 difference between two given colors, provided by color.el.

  Colors should be either hex rgb codes or simple HTML color names (recognized
  by color.el).

  More info: https://en.wikipedia.org/wiki/Color_difference"
  (color-cie-de2000 (apply 'color-srgb-to-lab (color-name-to-rgb color-code-1))
                    (apply 'color-srgb-to-lab (color-name-to-rgb color-code-2))))

(defun ntc--format-color-code (color-code)
  "Properly formats the color code: uppercase and with # in front"
  (let ((CODE (upcase color-code)))
    (cond
     ((= (length CODE) 7) CODE)
     ((and (= (length CODE) 6) (string-match-p "^[0-9A-F]+$" CODE)) (concat "#" CODE))
     (t CODE))))


(defun ntc--get-closest-color (color-code)
  "Returns a cons cell containing the matching grade and corresponding
ntc--struct of the closest color"
  (let* ((RGB (ntc--format-color-code color-code))
         (candidate (->> ntc--colors
                         (--annotate (ntc--compute-difference (ntc--struct-code it) RGB))
                         (--sort (< (car it) (car other)))
                         (-first-item))))
    candidate))

(defun ntc-name-this-color (color-code)
  "Returns the name of the color represented by color code or the name of the
closest alternative.

color-code should be a string naming a color (e.g. \"white\") or
a string specifying a color's RGB components (e.g. \"#ff12ec\", \"#fff\")."
  ;; return either the direct match
  (let* ((candidate (ntc--get-closest-color color-code))
         (name (ntc--struct-name (cdr candidate))))
    name))

(defun ntc-shade-this-color (color-code)
  "Returns the shade of the color represented by color code or the shade of the
closest alternative.

  Shade could be one of the following: Red, Orange, Brown, Yellow, Green, Blue,
  Violet, Gray, or White.

  color-code should be a string naming a color (e.g. \"white\") or
  a string specifying a color's RGB components (e.g. \"#ff12ec\", \"#fff\")."
  (let* ((candidate (ntc--get-closest-color color-code))
         (shade (ntc--struct-shade (cdr candidate))))
    shade))

